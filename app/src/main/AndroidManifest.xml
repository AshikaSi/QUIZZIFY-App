<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/trophy"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Quizzify"
        tools:targetApi="31">
        <activity android:name=".FirstActivity"
            android:exported="false"/>

        <activity android:name=".SecondActivity"
            android:exported="false"/>

        <activity android:name=".FetchActivity"
            android:exported="false"/>

        <activity android:name=".OneActivity"
            android:exported="false"/>

        <activity android:name=".InsertionActivity"
            android:exported="false"/>

        <activity android:name=".Questionslist"
            android:exported="false"/>

        <activity android:name=".QuizAdapter"
            android:exported="false"/>

        <activity android:name=".QuizDetailsActivity"
            android:exported="false"/>

        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>
